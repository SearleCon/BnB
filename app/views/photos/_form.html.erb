<h3>Upload photos</h3>
<%= simple_form_for Photo.new, :url => photos_path, :html => { :mutlipart => true } do |f| %>
    <%= f.error_notification %>
    <%= f.hidden_field :bnb_id, :value =>  @bnb.id %>

    <div class="form-inputs">
      <%= f.input :description %>
      <%= f.input :order %>
      <%= f.file_field :image %>
      <%= f.button :submit %>
    </div>
<% end %>
