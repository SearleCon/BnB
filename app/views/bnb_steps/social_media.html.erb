<div class="hero-unit">
<h2>Please fill in your social media details.</h2>

<%= render :layout => 'form' do |f| %>
    <div class = 'form-inputs'>
        <%= f.input :contact_person %>
        <%= f.input :twitter_account %>
        <%= f.input :facebook_page %>
    </div>
      <%= f.button :submit, :value => 'Finish', :id => 'commit', class: "btn btn-primary" %>
<%end%>
</div>
